{
  "name": "chunkjs",
  "version": "1.1.2",
  "description": "A minimal library for creating chunks of arrays in JavaScript",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "pretest": "npm run lint",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublish": "npm run build"
  },
  "files": ["dist/**/*"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrandall91/chunkjs.git"
  },
  "keywords": [
    "chunk",
    "array"
  ],
  "author": "Rashaad R. Randall <hello@rashaadrandall.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rrandall91/chunkjs/issues"
  },
  "homepage": "https://github.com/rrandall91/chunkjs#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
